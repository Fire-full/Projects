        ВПодтаблицу = Ложь;
        СтрокиАльтер= Nil;
        if ТипДокум="Справочники" then --Справочники
          DeleteSection (Позиции);
          ПодтаблицаДанных="";
          СекДокум1.Cell[5,1].List.Clear;
