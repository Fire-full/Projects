record Сотр_Выслуга title "";
  field Документ       :Zik_Гарнец.Документы.ЗапПриказВыслуга title "Документ о присвоении выслуги";
  field СОТР           :Базовый.Данные.Субъект                title "Полное наименование";
  field ДАТАПРИСВОЕНИЯ :DATE                                  title "ДАТА ПРИСВОЕНИЯ";
  field НОМЕРПРИКАЗА   :String                                title "НОМЕР ПРИКАЗА";
  field ПНВЛ           :Zik_Гарнец.Справочники.ПНВЛ           title "ЗВАНИЕ";
  field ВидНачисления  :Первичка.Справочники.ВидыНачислений   title "ВидыНачислений";
  field ВЫСЛУГАНАД     :DATE                                  title "ДАТА ПРИСВОЕНИЯ";
  field ВЫСЛУГАЛ       :INTEGER                               title "Л";
  field ВЫСЛУГАМ       :INTEGER                               title "М";
  field ВЫСЛУГАД       :INTEGER                               title "Д";
  field Метод          :INTEGER                               title "Метод";  --  0- "По среднему", 1- "На начало периода", 2- "На конец периода"
end;

