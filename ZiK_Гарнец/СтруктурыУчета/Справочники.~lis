
reference спрЗВАНИЯ title 'ЗВАНИЯ', from Справочники.ЗВАНИЯ, filter "Код<>'' and ТипЗаписи in [0, 1]", card Справочники.картЗВАНИЯ;
  field Код          synonym Имя      :String, from Код;
  field Наименование synonym Описание :String, notrefresh, from Имя;
end;
